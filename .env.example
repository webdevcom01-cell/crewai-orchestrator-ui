# Backend API Configuration
VITE_BACKEND_URL=http://localhost:8000/api

# Note: API keys are now stored securely on the backend server
# See server/.env.example for backend configuration

# ============ MONITORING ============

# Sentry Error Tracking
# Get your DSN from: https://sentry.io
VITE_SENTRY_DSN=

# Enable Sentry in development (optional)
VITE_SENTRY_DEV=false

# Application version for release tracking
VITE_APP_VERSION=1.0.0

# Analytics endpoint for Web Vitals (optional)
VITE_ANALYTICS_ENDPOINT=
